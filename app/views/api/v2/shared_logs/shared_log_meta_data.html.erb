        if (/windows phone/i.test(userAgent)){
            setTimeout(function () { window.location.href= "http://54.183.167.119:8000/logdetails/<%=@unique_code %>/<%= @log.id %>"; }, 25);
            window.location ="hairfolio_temp://ASDF/"+<%= @log.id %>
        }
        if (/android/i.test(userAgent)){
            setTimeout(function () { window.location = window.location.href= "http://54.183.167.119:8000/logdetails/<%=@unique_code %>/<%= @log.id %>"; }, 25);
            window.location ="hairfolio_temp://ASDF/"+<%= @log.id %>
        }
